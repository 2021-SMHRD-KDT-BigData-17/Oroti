<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jisang.poyong.mapper.NoticeMapper">

   <select id="LoadNotice" parameterType="String" resultType="jisang.poyong.vo.NoticeVO">
		SELECT companyname, noticejobcode, noticeregdate, noticeperiod 
		FROM noticeinfo
		ORDER BY noticeregdate DESC
	</select>
    
</mapper>